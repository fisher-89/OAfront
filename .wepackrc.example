{
  "entry": "src/index.js",
  "extraBabelPlugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "define": {
    "OA_PATH": "",
    "OA_CRM_UPLOAD": "",
    "OA_CLIENT_ID": "",
    "OA_CLIENT_SECRET": "",
    "AUTH_NAME": "",
    "TOKEN_PREFIX": ""
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ],
      "define":{
        "OA_PATH": "",
        "OA_CRM_UPLOAD": "",
        "OA_CLIENT_ID": "",
        "OA_CLIENT_SECRET": ""
      }
    },
    "production": {
      "define": {
        "OA_PATH": "",
        "OA_CRM_UPLOAD": "",
        "OA_CLIENT_ID": "",
        "OA_CLIENT_SECRET": ""
      }
    }
  },
  "ignoreMomentLocale": true,
  "theme": "./src/theme.js",
  "html": {
    "template": "./src/index.ejs"
  },
  "publicPath": "/",
  "disableDynamicImport": true,
  "hash": true
}
